{
  "name": "@syntest/solidity",
  "version": "0.0.1",
  "description": "SynTest Solidity is a tool for automatically generating test cases for the Solidity platform",
  "keywords": [
    "syntest",
    "automatic-test-generation"
  ],
  "homepage": "https://www.syntest.org",
  "bugs": {
    "url": "https://github.com/syntest-framework/syntest-solidity/issues"
  },
  "license": "MIT",
  "contributors": [
    "Annibale Panichella",
    "Mitchell Olsthoorn",
    "Dimitri Stallenberg"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "syntest-solidity": "dist/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syntest-framework/syntest-solidity.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "npm run build:compile",
    "build:compile": "tsc --build",
    "build:watch": "tsc --build --watch",
    "clean": "rm -rf .nyc_output dist node_modules",
    "clean:dist": "rm -rf dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "npm run build",
    "run": "truffle run syntest-solidity",
    "test": "mocha",
    "test:coverage": "nyc mocha",
    "test:watch": "mocha --watch"
  },
  "dependencies": {
    "@solidity-parser/parser": "^0.6.0",
    "@truffle/config": "^1.2.44",
    "@truffle/provider": "^0.1.17",
    "bignumber.js": "^9.0.1",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "detect-port": "^1.3.0",
    "figlet": "^1.5.0",
    "fs-extra": "^8.1.0",
    "ganache-cli": "6.9.0",
    "ghost-testrpc": "^0.0.2",
    "global-modules": "^2.0.0",
    "globby": "^10.0.1",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.15",
    "mocha": "^8.3.1",
    "node-emoji": "^1.10.0",
    "original-require": "^1.0.1",
    "pify": "^4.0.1",
    "recursive-readdir": "^2.2.2",
    "sc-istanbul": "^0.4.5",
    "shelljs": "^0.8.3",
    "solidity-coverage": "^0.7.16",
    "@syntest/framework": "file:../syntest-framework",
    "truffle": "^5.1.65",
    "web3": "^1.3.4",
    "web3-utils": "^1.5.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.4",
    "@types/sinon": "^9.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chai": "^4.3.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.1.0",
    "mocha": "^8.3.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "sinon": "^9.2.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=10.24.0"
  }
}
